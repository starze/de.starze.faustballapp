<?xml version='1.0' encoding='utf-8'?>
<widget id="de.starze.faustballapp" version="0.5.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FaustballApp</name>
    <description>
        App zum Auffinden der wichtigsten Faustball-Webseiten.
    </description>
    <author email="oeffentlichkeitsarbeit@stb-faustball.de" href="http://stb-faustball.de">
        Daniel Starzmann
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="22" />
        <preference name="android-targetSdkVersion" value="29" />
        <icon src="www/img/faustball_icon_36.png" density="ldpi" />
        <icon src="www/img/faustball_icon_48.png" density="mdpi" />
        <icon src="www/img/faustball_icon_72.png" density="hdpi" />
        <icon src="www/img/faustball_icon_96.png" density="xhdpi" />
        <icon src="www/img/faustball_icon_144.png" density="xxhdpi" />
        <icon src="www/img/faustball_icon_192.png" density="xxxhdpi" />
    </platform>
</widget>
