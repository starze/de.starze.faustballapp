<?xml version='1.0' encoding='utf-8'?>
<widget id="de.starze.faustballapp" version="0.4.0" versionCode="4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FaustballApp</name>
    <description>
        App zum Auffinden der wichtigsten Faustball-Webseiten.
    </description>
    <author email="oeffentlichkeitsarbeit@stb-faustball.de" href="http://stb-faustball.de">
        Daniel Starzmann
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <preference name="android-targetSdkVersion" value="28" />
        <icon density="ldpi" src="www/img/faustball_icon_36.png" />
        <icon density="mdpi" src="www/img/faustball_icon_48.png" />
        <icon density="hdpi" src="www/img/faustball_icon_72.png" />
        <icon density="xhdpi" src="www/img/faustball_icon_96.png" />
        <icon density="xxhdpi" src="www/img/faustball_icon_144.png" />
        <icon density="xxxhdpi" src="www/img/faustball_icon_192.png" />
    </platform>
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <engine name="android" spec="^7.1.1" />
</widget>
